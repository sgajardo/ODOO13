<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="seq_real_project" model="ir.sequence">
            <field name="name">Ticket</field>
            <field name="code">ticket.pro</field>
             <field name="prefix">TKT/%(range_year)s/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>

    <data noupdate="1">
        <record id="seq_change_project" model="ir.sequence">
            <field name="name">Change</field>
            <field name="code">change.project</field>
             <field name="prefix">DSR/%(range_year)s/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>

    <data noupdate="1">
        <record id="seq_ticket_note" model="ir.sequence">
            <field name="name">Notes</field>
            <field name="code">ticket.notes</field>
             <field name="prefix">NOT/%(range_year)s/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>
